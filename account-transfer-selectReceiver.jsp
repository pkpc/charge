<script>
console.log(1);
createHisReceiver(hisReceiver_data,2);
search_eventHandler();
function search_eventHandler(){
    //设置搜索框样式
    $(".icon-box").off("click").click(function(){
        $("input.search-input").addClass("search-input-active");
        $("input.search-input").focus();
        $(".icon-box").addClass("icon-box-active");
        $("#search span").html("");
    });
    $("input.search-input").off("blur").blur(function(){
        if($(this).val()==""){
            $("input.search-input").removeClass("search-input-active");
            $(".icon-box").removeClass("icon-box-active");
            $("#search span").html("搜索");
        }
    });
    $("#account-transfer-Page4 #search input").off("input propertychange").on("input propertychange",function(){
        var searchInfo = $(this).val();
        if(searchInfo!=""){
            searchReceiver(searchInfo);
        }else{
            createHisReceiver(hisReceiver_data,2);
            $(".his-box .receiver").siblings().css("display","block");
        }
    });

}

function searchReceiver(searchInfo){
    var info = hisReceiver_data;
    var Search_result =false;
    var html="";
    //搜索手机前3位，末4位，名字，公司名字
    $.each(info,function(){
        var phone = this.phone.split("#")[0];
        var phoneStart = phone.substring(0,3);
        var phoneEnd = phone.slice(-4);
        if(findString(this.name,searchInfo)||findString(phoneStart,searchInfo)||findString(this.companyName,searchInfo)||findString(phoneEnd,searchInfo)){
            html += createOneHisReceiver(this.name,phone,phoneStart,phoneEnd,this.companyName);
            Search_result=true;
        }
    });
    if(Search_result){
        $(".his-box .receiver").html(html);
        $(".his-box .receiver").siblings().css("display","block");
    }else{
        html ='<div id="search-none">';
        html+= '<img src="../../images/charge/transfer-search-none.png" alt="">'
        html+='<p class="ftsize15" style="color: #ccc;">无搜索结果</p>'
        html+='</div>';
        $(".his-box .receiver").html(html);
        $(".his-box .receiver").siblings().css("display","none");
    }
}
function createOneHisReceiver(name,phone,phoneStart,phoneEnd,companyName){
    var str='';
    str = '<a class="his-item" onclick="loadHisReceiver(this);history.go(-1);">';
    str+='<div class="his-item-box ftsize14">';
    str+='<span class="his-name">' + name + '</span>';
    str+='<span class="his-tel">'+phoneStart+'****'+phoneEnd +'</span>';
    str+='<span class="his-tel-hidden" style="display:none">'+phone+'</span>';
    str+='</div>';
    str+='<div class="his-toCompanyName ftsize13">'+companyName+'</div>';
    str+='<img src="../../images/charge/arrow.png" alt="arrow">';
    str+='</a>';
    return str;
}
